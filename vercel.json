{
  "version": 2,
  "name": "german-school-holidays-api",
  "functions": {
    "index.js": {
      "maxDuration": 10,
      "includeFiles": "routes/years/**, docs/**, public/**"
    }
  },
  "routes": [
    {
      "src": "/(health|status|ready)",
      "dest": "/index.js"
    },
    {
      "src": "/docs",
      "dest": "/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
